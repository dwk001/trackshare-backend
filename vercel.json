{
  "version": 2,
  "name": "trackshare",
  "functions": {
    "api/resolve.js": {
      "maxDuration": 30
    },
    "api/t.js": {
      "maxDuration": 10
    },
    "api/providers.js": {
      "maxDuration": 30
    },
    "api/activity.js": {
      "maxDuration": 30
    },
    "api/privacy.js": {
      "maxDuration": 30
    },
    "api/auth/google/callback.js": {
      "maxDuration": 30
    },
    "api/auth/apple/callback.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/t/:id",
      "destination": "/api/t?id=:id"
    },
    {
      "source": "/auth/google/callback",
      "destination": "/api/auth/google/callback"
    },
    {
      "source": "/auth/apple/callback", 
      "destination": "/api/auth/apple/callback"
    }
  ]
}