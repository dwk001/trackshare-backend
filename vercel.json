{
  "version": 2,
  "name": "trackshare",
  "functions": {
    "api/resolve.js": {
      "maxDuration": 30
    },
    "api/t.js": {
      "maxDuration": 10
    },
    "api/providers.js": {
      "maxDuration": 30
    },
    "api/activity.js": {
      "maxDuration": 30
    },
    "api/privacy.js": {
      "maxDuration": 30
    },
            "api/trending.js": {
                "maxDuration": 60
            },
            "api/search.js": {
                "maxDuration": 30
            },
    "api/auth/google/callback.js": {
      "maxDuration": 30
    },
            "api/auth/apple/callback.js": {
              "maxDuration": 30
            },
            "api/posts.js": {
              "maxDuration": 30
            },
            "api/friends.js": {
              "maxDuration": 30
            },
            "api/profile.js": {
              "maxDuration": 30
            }
  },
  "crons": [{
    "path": "/api/trending?refresh=true",
    "schedule": "0 2 * * *"
  }, {
    "path": "/api/trending?seed=true",
    "schedule": "0 3 * * *"
  }],
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/t/:id",
      "destination": "/api/t?id=:id"
    },
    {
      "source": "/auth/google/callback",
      "destination": "/api/auth/google/callback"
    },
    {
      "source": "/auth/apple/callback", 
      "destination": "/api/auth/apple/callback"
    }
  ]
}